for i in {0..15}; 
do 
    python3 rmm.py $((2**i));
    cargo r --release $((2**i));
    DIFF=$(diff output.txt rust_output.txt)
    if [ "$DIFF" != "" ]
    then
        echo "WRONG"
        mkdir -p failed_tests
        diff -y output.txt rust_output.txt > failed_tests/sz$((2**i)).txt
    else
        echo "correct"
    fi
done